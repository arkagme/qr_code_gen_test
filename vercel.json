{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/r/(.*)",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "index.html"
    }
  ]
}

{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/r/(.*)",
      "dest": "api/routes/redirect.js"
    },
    { 
      "handle": "filesystem" 
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}

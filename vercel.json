{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/r/(.*)",
      "dest": "public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
